OL.numbers{
        list-style: none;
        counter-reset: my-awesome-counter;
        margin-left: 0;
        margin-top: 32px;
    LI{
        counter-increment: my-awesome-counter;
        position: relative;
        padding-left: 36px;
        margin-bottom: 12px;
        line-height: 26px;
    }
    li::before {
        color: $blue;
        font-family: $body-stack-bold;
            content: counter(my-awesome-counter);
        border-radius: 70px;
        border: 1px solid #38414f;
        font-size: 14px;
        padding: 2px 7px 0 8px;
        margin-right: 15px;
        position: absolute;
        top: 0;
        left: 0;
        line-height: 20px;
    }
}

