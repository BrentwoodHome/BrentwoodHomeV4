Body{
    //overflow: hidden;
}
.product-shop .short-description{
    line-height: 30px;
    letter-spacing: 1px;
}

abbr {
    position: relative;
}

abbr:hover::after {
    position: absolute;
    bottom: 100%;
    left: 100%;
    display: block;
    padding: 1em;
    background: yellow;
    content: attr(title);
}
.wow{
    //display: none;
}
h2{
    font-size: 36px;
    line-height: 48px;
    font-style: italic;
    text-align: left;
    margin-bottom: 32px;
    text-transform: capitalize;
}
h2.main{
    line-height: 66px;
    padding: 0 15%;
}
h3.features{
    margin: 24px 0;
    font-size: 21px;
    color: $blue;
    text-transform: none;
    text-align: center !important;
}
p{
    line-height: 30px;
}
.container.section{
    A{
        text-decoration: underline;
        &:HOVER{
            text-decoration: none;
        }
    }
    STRONG{
        color: #555;
    }
}
.swatch label.swatch-label{
    border-radius: 6px;
    line-height: 33px;
    border-width: 1px;
    padding: 0 10px;
}
.swatch input:checked + label.swatch-label, .swatch label.swatch-label:hover{
    border-color: $lightblue;
    line-height: 33px;
    border-radius: 6px;
    background-color: #687283;
    color: white;
    border-width: 1px;
}

    LEGEND{
        font-size: 14px;
        color: #999;
        font-style: italic;
    }
.section{
    margin-top: 20px;
    margin-bottom: 20px;
    &.cc_yoga{
        DIV.image{
            background-image: url(https://s3-brentwoodhome.imgix.net/Crystal_Cove%20Family.jpg?rect=0,1260,7190,3530&h=560);
            padding: 50px 0 150px;
            height: 560px;
        }
        text-align: center;
        color: white;
        height: 560px;
        H3{
            text-transform: uppercase !important;
            color: $darkgrey !important;
            text-align: center !important;
        }
        P{
            color: #666;
            A{
                &:HOVER{
                    text-decoration: underline;
                }
            }
        }
    }
    &.mountains{
        DIV.image{
            background-image: url(https://s3-brentwoodhome.imgix.net/products/cedarluxe/indialandscape2.jpg?w=1140&q=60);
            padding: 50px 0 150px;
            height: 560px;
            background-position: center bottom;
        }
        text-align: center;
        color: white;
        height: 560px;
        H3{
            text-transform: uppercase !important;
            color: #fff !important;
            font-size: 26px !important;
            margin: 28px 0 !important;
        }
        P{
            color: #fff;
            font-size: 18px;
            A{
                &:HOVER{
                    text-decoration: underline;
                }
            }
        }
    }
    &.cc_coils{
        DIV.image{
            background-image: url(https://s3-brentwoodhome.imgix.net/5zonecoils.jpg?rect=0,0,7650,3890&h=620);
            padding: 100px 0 150px;
            height: 620px;
        }
        text-align: left;
        color: white;
        height: 620px;
        .drop{
            background-color: rgba(88,88,88,0.6);
            padding: 30px;
        }
        H3{
            color: white !important;
            font-size: 24px !important;
            text-align: left !important;
        }
        P{
            color: white;
        }
    }
}
.flex{
    display: flex;
}
.flexV {
    display: flex;
    align-items: center;
    justify-content: center;
}
.flexV div{
    padding-left: 35px;
    padding-right: 35px;
}
IMG.rdthumb{
    border: 5px solid white;
    box-shadow: 0 0 7px rgba(0,0,0,0.5);
    border-radius: 500px;
    -webkit-border-radius: 500px;
    -moz-border-radius: 500px;
}
IMG.rdrectthumb{
    border: 5px solid white;
    box-shadow: 0 0 7px rgba(0,0,0,0.5);
    border-radius: 10px;
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
}
LABEL.selectLabel{
    color: #38414f;
    font-size: 16px !important;
    font-weight: 300 !important;
    letter-spacing: 0.15px !important;
    line-height: 27px;
    font-family: AvenirLTStd-Roman,"Helvetica Neue",helvetica,sans-serif;
}
.side{
    text-align: center;
    position: relative;
    height: 100%;
    DIV.top{
        border-radius: 5px;
        height: 250px;
        position: relative;
        padding: 20px;
    }
    DIV.thumb{
        position: absolute;
        bottom: -120px;
        margin-left: -120px;
        left: 50%;
    }
    DIV.thumb2{
        margin-top: 30px;   
    }
    h3{
        color: white !important;
        font-size: 24px !important;
        letter-spacing: 8px !important;
        text-transform: uppercase !important;
        margin: 10px 0 !important;
        text-align: center !important;
    }
    h4{
        color: white;
        font-size: 24px;
        font-weight: 100;
        margin: 15px 0;
        letter-spacing: 4px;
        line-height: 35px;
    }
    &.charcoal .top{
        background-color: #535151;
    }
    &.latex .top{
        background-color: #A7A59A;
    }
    .content{
        color: white;
        padding: 20px 40px;
        background-color: #EEEEEE;
        margin-top: 20px;
        border-radius: 5px;
        padding-top: 0px;
        height: 70%;
        &.bamboo{
            //height: 40%;
        }
        p, h4{
            color: #666;
        }
        padding-top: 110px;
    }
}
UL.nav.nav-tabs{
    LI{
        margin: 20px 20px 0;
        A{
            padding: 20px;
        }
    }
    LI.active{
        A{
            background-color: #f9f9f9;
        }
    }
}
DIV.tab-content{
    padding: 35px;
    background-color: #f9f9f9;
    border-radius: 6px;
    margin-top: -1px;
    border: 1px solid #ddd;
    DIV.tab-pane{
        .col-xs-12{
            margin-top: 10px;
            margin-bottom: 10px;
        }
        P STRONG{
            font-weight: 300;
            font-size: 18px;
        }
    }
}

