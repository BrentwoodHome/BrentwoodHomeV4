$body-text-color: #808080;
$dark-text-color: #4a4a4a;
$semidark-text-color: #6b6b6b;
$blue: #38414F;
$lightblue: #454d5a;
$mauve: #A28F94;
$grey: #A4A6A3;
$darkgrey: #444;
$darkgreen: #6D7A68;
