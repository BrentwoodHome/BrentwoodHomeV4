#myModalSale{
    .modal-header{
        padding: 45px;
        border: none;
    }
    .modal-header .close{
        font-size: 50px;
        position: absolute;
        top: 0;
        right: 20px;
        z-index: 2000;
    }
    .modal-dialog{
        top: 25%;
    }
    .modal-body{
        padding: 0;
    }
    .copy{
        padding-right: 30px;
    }
    H2{
        @include titleStyle(42px);
        font-style: italic;
        font-size: 38px;
        line-height: 46px;
        text-align: center !important;
    }
    H3{
        margin: 15px 0 30px;
        font-size: 18px;
    }
    P.promocode{
        display: inline-block;
        background-color: $blue;
        padding: 12px;
        font-weight: bold;
        font-size: 24px;
        color: white;
    }
    #myModalSale .img{
        overflow: hidden;
        width: 200px;
        height: 500px;
    }
}
@media(max-width:768px){
    #myModalSale .img{
        display: none;
    }
    #myModalSale .modal-header{
        padding: 15px;
    }
    #myModalSale .copy{
        padding-right: 15px;
        padding-bottom: 20px;
    }
}
