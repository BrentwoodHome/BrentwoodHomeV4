@mixin color-btn($color){
    .btn{
        @include darken($color, 30%);
        border: none;
        line-height: 1px;
        //padding: 25px 40px;
        padding: 25px 0px;
        width: 100%;
        &:HOVER{
            @include darken($color, 40%)
                }
    }
}
