@mixin chronicleHairline($size){
    font-family: "Chronicle Hairline A", "Chronicle Hairline B";
    font-style: italic;
    font-weight: 400;
    font-size: $size;
    letter-spacing: 2px;
    color: $darkgrey;
    text-rendering: optimizeLegibility; 
    text-transform: none; // mac ff fix
    -webkit-font-smoothing: antialiased; // safari mac nicer
    -moz-osx-font-smoothing: grayscale; // fix fatty ff on mac
}
@mixin chronicleDisplay($size){
    font-family: "Chronicle Display A", "Chronicle Display B";
    font-style: italic;
    font-weight: 200;
    font-size: $size;
    color: $darkgrey;
    text-rendering: optimizeLegibility; 
    text-transform: none; // mac ff fix
    -webkit-font-smoothing: antialiased; // safari mac nicer
    -moz-osx-font-smoothing: grayscale; // fix fatty ff on mac
}

@mixin fontrendering(){
    text-rendering: optimizeLegibility; 
    //text-transform: none; // mac ff fix
    -webkit-font-smoothing: antialiased; // safari mac nicer
    -moz-osx-font-smoothing: grayscale; // fix fatty ff on mac
}

STRONG{
    font-family: $body-stack-bold;
    //font-weight: 700;
    text-rendering: optimizeLegibility; 
    text-transform: none; // mac ff fix
    -webkit-font-smoothing: antialiased; // safari mac nicer
    -moz-osx-font-smoothing: grayscale; // fix fatty ff on mac
}
@mixin titleStyle($size){
    line-height: $titleSize*1.4;
    //font-size: $titleSize;
    font-weight: 400;
    color: #4A4A4A;
    //text-transform: uppercase;
    letter-spacing: 2px;
    font-family: $title-stack;
}

@mixin subTitleStyle($size){
    font-family: $body-stack-bold;
    font-size: $size;
    line-height: $size*1.5;
    color: $semidark-text-color;
    text-transform: uppercase;
    font-weight: normal;
    margin: 6px 0;
}

@mixin quoteStyle{
    font-size: 24px;
    text-align: center;
    letter-spacing: 1px;
    font-family: $body-stack-oblique;
    //font-style: italic;
    font-weight: normal;
    color: #4a4a4a;
}

@mixin flexVcenter{
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -ms-flex-align: center;
    -webkit-align-items: center;
    -webkit-box-align: center;
    align-items: center;
    justify-content: center;
}

