    }
    .container.section.margTBLarge{
    margin-top: 80px;
    margin-bottom: 80px;
    }*/
    @media screen and (max-width:768px) {
        .flex{
            display: block;
        }
        .hero-text {
            text-align: center;
            position: absolute;
            top: 50%;
            left: 20%;
            transform: translate(-10%, -50%);
            color: white;
        }
        .hero-text H1{
            color: white;
            font-size: 36px !important;
            margin-bottom: 20px !important;
        }
        .standardPages>.row{
            margin-top: 50px;
            margin-bottom: 50px;
        }
        .template-page .standardPages .section h2{
            margin-top: 40px;
        }
        .margTBLarge{
            margin-top: 60px;
            margin-bottom: 60px;
        }
        .breadcrumb-wrap{
            display: none;
        }
        .sectionHighlight{
            margin-top: 40px;
            margin-bottom: 40px;
            padding: 60px 0;
        }
        .hero-image{
            height: 42vh;
        }
        P.reliefLogo IMG{
            max-width: 40%;
            margin: 20px auto;
        }
    }
</style>

