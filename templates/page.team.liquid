<style>
    .template-page .standardPages h2, .template-page .standardPages h3{
        text-align: left;
    }
    .standardPages>.row{
        margin-top: 100px;
        margin-bottom: 100px;
    }
    .template-page .standardPages h2{
        margin-top: 0;
    }
    I.fa.affirm{
        color: #00c9e5;
    }
    UL LI{
        line-height: 40px;
    }
    OL LI{
        line-height: 27px;
        padding-bottom: 10px;
    }
    strong{
        color: #666;
    }
    .text-small{
        line-height: 20px !important;
    }
    .underline{
        text-decoration: underline;
    }
    .underline:HOVER{
        text-decoration: none;
    }
    .flexV div{
        padding-left: 15px !important;
        padding-right: 15px !important;
    }
    /* The hero image */
    .hero-image {
        /* The image used */
        background-image: url("https://brentwood-home-2.imgix.net/bhfamily.jpg?7788353565071076423");

        /* Set a specific height */
        height: 62vh;

        /* Position and center the image to scale nicely on all screens */
        background-position: center top;
        background-repeat: no-repeat;
        background-size: cover;
        position: relative;
    }

    /* Place text in the middle of the image */
    .hero-text {
        text-align: center;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        color: white;
    }
    .hero-text H1{
        color: white;
        font-size: 70px !important;
        /*margin-bottom: 20px !important;*/
        margin: 0;
    }
    .hero-text H3{
        color: white;
        font-size: 20px !important;
        font-style: normal !important;
    }
    H2.sectiontitle{
        text-align: center !important;
        font-size: 36px;
    }
    .margTBLarge{
        margin-top: 120px;
        margin-bottom: 120px;
    }
    .sectionHighlight{
        background-color: #F9F4F4;
        padding: 150px 0;
    }
    P.reliefLogo IMG{
        max-width: 80%;
    }
    .section.team H2{
        margin-bottom: 15px; 
    }
    .section.team H3{
        margin-bottom: 15px; 
        font-size: 18px;
    }
