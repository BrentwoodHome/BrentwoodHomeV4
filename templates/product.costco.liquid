
<style>
  #accordion H2{
    text-align: left;
    font-size: 21px;
  }
  .product-shop #add-to-cart-form #product-add-to-cart, .product-shop A.cta4amz{
    line-height: 70px;
    padding: 0;
  }
</style>

<div class="container-fluid" id="product">
  <div itemscope itemtype="http://schema.org/Product" class="product">
    <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
    <meta itemprop="image" src="{{ product.featured_image.src | product_img_url: 'grande' }}" alt="{{ product.title | escape }}">
    <meta itemprop="itemCondition" content="http://schema.org/NewCondition" />
    <div class="row">
      {%comment%}
      {% include 'product-flexslider' %}
      to remove the pull-left for single products
      {%endcomment%}
      <style>
    .flexslider .slides > li{
        background-color: #f9f9f9;
    }
    .embed-container { 
        position: relative;
        left: 0; 
        right: 0;
        top: 50%;
        /* video height / video width */
        /*padding-bottom: 66.2%;*/
        /* the above value * 0.5 */
        /*margin-top: -28.1%;*/
        height: 0;
        overflow: hidden;
    } 
    .embed-container iframe, .embed-container object, .embed-container embed { 
        position: absolute; 
        top: 0; 
        left: 0; 
        width: 100%; 
        height: 100%; 
    }
</style>

{% assign featured_image = product.selected_or_first_available_variant.featured_image | default: product.featured_image %}
{% assign imgName = featured_image | remove: 'products/' %}
<div class="col-xs-12 col-md-7 col-lg-7" id="product-image-main">
    <section class="slider" style="position: relative;">
        {% if sale %}
        <div class="sale-badge-wrap testB">
            <div class="sale-badge">
                <p>{{ sale }}</p>
            </div>
        </div>
        {% endif %}
        <!-- -->
        <div id="flexPlaceholder">
            <picture>
                <source media="(max-width: 767px)" data-srcset="https://brentwood-home-products.imgix.net/{{ imgName }}?ch=Width,DPR&w=767&dpr=1&fit=clip">
                <source media="(min-width: 768px)" data-srcset="https://brentwood-home-products.imgix.net/{{ imgName }}?ch=Width,DPR&w=1024&dpr=1&fit=clip">
                <img src="https://brentwood-home-products.imgix.net/{{ imgName }}?w=1024&q=50" alt="{{product.title}}" alt="{{product.title}}" loading="eager">
            </picture>
        </div>
      <div class="flexslider">
                            <ul class="slides">
                                {% for image in product.images %}
                                {% assign videotag = image.alt | escape %}
                                    {% assign imgName = image | remove: 'products/' | split:'?' | first %}
                                <li data-thumb="https://brentwood-home-products.imgix.net/{{ imgName }}?ch=Width,DPR&fp-x=0.5&fp-y=0.5&fp-z=1&fp-debug=false&w=160&h=120&crop=focalpoint&dpr=1 1x,
                                    https://brentwood-home-products.imgix.net/{{ imgName }}?ch=Width,DPR&crop=focalpoint&fp-x=0.5&fp-y=0.5&fp-z=1&fp-debug=false&w=160&h=120&fit=crop&dpr=2 2x,
                                    https://brentwood-home-products.imgix.net/{{ imgName }}?ch=Width,DPR&crop=focalpoint&fp-x=0.5&fp-y=0.5&fp-z=1&fp-debug=false&w=160&h=120&fit=crop&dpr=3 3x">
                                    {% if videotag contains 'vimeo' %}
                                        <div class='embed-container '>
                                            <!--https://player.vimeo.com/video/###-->
                                            <iframe src="{{ image.alt }}?title=0&byline=0&portrait=0" width="640" height="360" id="video_1" frameborder="0"></iframe>
                                        </div>
