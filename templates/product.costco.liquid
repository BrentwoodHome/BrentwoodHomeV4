{% comment %}
{% if product.metafields.bhsite.materials %}
  <section class="overlays product-tabs overlays-tabs container">
      <div id="exTab1" class="">
          <h2 class="overlays-title">Product Description</h2>
          <ul class="nav nav-pills">
              <li class="active">
                  <a href="#1a" data-toggle="tab">Description</a>
              </li>
              <li>
                  <a href="#2a" data-toggle="tab">Materials</a>
              </li>
          </ul>
          <div class="tab-content clearfix">
              <div class="tab-pane active" id="1a">
                  <div class="description text-left">
                      {{ product.description }}
                  </div>
              </div>
              <div class="tab-pane" id="2a">
                  <div class="description text-left">
                      {{ product.metafields.bhsite.materials }}
                  </div>
              </div>
          </div>
      </div>
  </section>
{% else %}
  <div class="product-tabs container">
    <div class="row">
      <div class="col-xs-12 col-sm-8 col-sm-offset-2">
        <h2>Product Description</h2>
        <div id="myTabContent" class="tab-content">
          {% if settings.display_product_detail_description %}
          <div id="collapse-tab1" class="tab-pane fade in active">
            {{ product.description }}
          </div>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
{% endif %}
{% endcomment %}

<div class="container section margTB120">
    <h2 class="text-center">Product Specifications</h2>
    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="headingZero">
                <h4 class="panel-title">
                    <a role="button" class="icons prddescription" data-toggle="collapse" href="#collapseZero" aria-expanded="true" aria-controls="collapseZero">
                        Description
                    </a>
                </h4>
            </div>
            <div id="collapseZero" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingZero">
                <div class="panel-body">
                    {{ product.description }}
                </div>
            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="headingOne">
                <h4 class="panel-title">
                    <a role="button" class="icons certifications" data-toggle="collapse" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
                        Materials
                    </a>
                </h4>
            </div>
            <div id="collapseOne" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
                <div class="panel-body">
                    {{ product.metafields.bhsite.materials }}
                </div>
            </div>
        </div>
    </div>
</div>



