{{ 'jquery.sticky.js' | asset_url | script_tag }}
<script>
    $(document).ready(function(){
        $(window).scroll(function () {
            //console.log($(window).scrollTop());
            if (window.innerWidth > 414) {
                if ($(window).scrollTop() > 500) {
                    $("#followMeBar").sticky({
                        topSpacing:0,
                        zIndex: 10000
                    });
                    $('#followMeBar').removeClass('stickybuy-invisible');
                    $('#followMeBar-sticky-wrapper').removeClass('stickybuy-invisible');
                    //$('#followMeBar').addClass('stickybuy-visible');
                }
                if ($(window).scrollTop() < 501) {
                    $('#followMeBar').addClass('stickybuy-invisible');
                    $('#followMeBar-sticky-wrapper').addClass('stickybuy-invisible');
                }
            }else{
                if ($(window).scrollTop() > 1050) {
                    $("#followMeBar").sticky({
                        topSpacing:0,
                        zIndex: 10000
                    });
                    $('#followMeBar').removeClass('stickybuy-invisible');
                    $('#followMeBar-sticky-wrapper').removeClass('stickybuy-invisible');
                }
                if ($(window).scrollTop() < 1051) {
                    $('#followMeBar').addClass('stickybuy-invisible');
                    $('#followMeBar-sticky-wrapper').addClass('stickybuy-invisible');
                }
            }
        });
        //scroll back to CTA
        $('A.sscroll').bind('click', function(event) {
            var $anchor = $(this);
            $('html, body').stop().animate({
                scrollTop: $($anchor.attr('href')).offset().top - 150
            }, 1500, 'easeInOutExpo');
            event.preventDefault();
        });
    });
</script>

