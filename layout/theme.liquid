        <script>
            //eraseCookie("bwh_impact");
            var myImpactCookie = getCookie("bwh_impact");

            // if clickid present in url, always update cookie with latest value, if no clickid but cookie present, dont do anything, if no click id and no cookie, generate clickid
            var impact_clickID = "";
            const urlParams = new URLSearchParams(window.location.search);
            if(urlParams.has('irclickid')){
                //clickid available from url
                //console.log(urlParams.get('irclickid')); // true 
                impact_clickID = urlParams.get('irclickid');
                setCookie("bwh_impact", impact_clickID, 120); //create 120 day cookie with url value
            }else{
                //no value in url, check if cookie doesn't exists, if not, do nothing
                if (myImpactCookie == null) {
                    //cookie doesn't exists, no click id available, generate one
                    ire('generateClickId', function(clickId) {
                        // use clickId here
                        //console.log("generated clickID: ", clickId);
                        setCookie("bwh_impact", clickId, 120);
                    });
                    //impact_clickID = generateClickId();
                    //setCookie("bwh_impact", impact_clickID, 120);
                }
            }
