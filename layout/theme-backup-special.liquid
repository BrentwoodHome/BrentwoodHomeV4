      <script>
          var CRL8_TRANSACTION = {
              'currency': 'USD',
              'items': [{
                  'id': '{{product.id}}',
                  'name': '{{product.title}}',
                  'quantity': 1,
                  'price': '{{ product.price | money_without_currency }}',
                  'displayPrice': '{{ product.price | money_without_currency }}',
                  'category': '{{ collection.title }}'
              }]
          };
          // end data

          // add PDP event
          crl8('viewProduct', {
              'productId': CRL8_TRANSACTION.items[0].id,
              'productName': CRL8_TRANSACTION.items[0].name,
              'price': CRL8_TRANSACTION.items[0].price,
              'displayPrice': CRL8_TRANSACTION.items[0].displayPrice,
              'quantity': CRL8_TRANSACTION.items[0].quantity,
              'category': CRL8_TRANSACTION.items[0].category
          });

          // send to Curalate
          crl8('send');
          
          $(document).ready(function(){
              $("#product-add-to-cart").click(function(){
                  crl8('addToCart', {
                      'productId': CRL8_TRANSACTION.items[0].id,
                      'productName': CRL8_TRANSACTION.items[0].name,
                      'price': CRL8_TRANSACTION.items[0].price,
                      'displayPrice': CRL8_TRANSACTION.items[0].displayPrice,
                      'quantity': CRL8_TRANSACTION.items[0].quantity,
                      'category': CRL8_TRANSACTION.items[0].category
                  });
                  crl8('send');
              });
          });
      </script>
